<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:check="using:GetStoreApp.Converters.Checks"
    xmlns:convert="using:GetStoreApp.Converters.Conversions"
    xmlns:format="using:GetStoreApp.Converters.Formats">

    <check:FilterValueCheckConverter x:Name="FilterValueCheckConverter" />
    <check:TimeSortValueCheckConverter x:Name="TimeSortValueCheckConverter" />

    <convert:BooleanToVisibilityConverter x:Name="BooleanToVisibilityConverter" />
    <convert:BooleanToReverseVisibilityConverter x:Name="BooleanToReverseVisibilityConverter" />
    <convert:EnumToBooleanConverter x:Key="EnumToBooleanConverter" />
    <convert:IntToVisibilityConverter x:Name="IntToVisibilityConverter" />
    <convert:IntToReverseVisibilityConverter x:Name="IntToReverseVisibilityConverter" />

    <format:CategoryIDInfoFormatConverter x:Name="CategoryIDInfoFormatConverter" />
    <format:ChannelNameFormatConverter x:Name="ChannelNameFormatConverter" />
    <format:GMTFormatConverter x:Name="GMTFormatConverter" />
    <format:HistoryCountInfoFormatConverter x:Name="HistoryCountInfoFormatConverter" />
    <format:ResultCountInfoFormatConverter x:Name="ResultCountInfoFormatConverter" />
    <format:TimeStampFormatConverter x:Name="TimeStampFormatConverter" />
    <format:TypeNameFormatConverter x:Name="TypeNameFormatConverter" />
</ResourceDictionary>
