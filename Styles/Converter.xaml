<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:GetStoreApp.Converters">

    <converter:CheckNullConverter x:Name="CheckNullConverter" />
    <converter:BooleanToVisibilityConverter x:Name="BooleanToVisibilityConverter" />
    <converter:BooleanToReverseVisibilityConverter x:Name="BooleanToReverseVisibilityConverter" />
    <converter:EnumToBooleanConverter x:Key="EnumToBooleanConverter" />
    <converter:IntToVisibilityConverter x:Name="IntToVisibilityConverter" />
    <converter:IntToReverseVisibilityConverter x:Name="IntToReverseVisibilityConverter" />
    <converter:DataTimeFormatConverter x:Name="DataTimeFormatConverter" />
    <converter:TypeNameConverter x:Name="TypeNameConverter" />
    <converter:ChannelNameConverter x:Name="ChannelNameConverter" />
    <converter:UtcToLocalTimeConverter x:Name="UtcToLocalTimeConverter" />
</ResourceDictionary>
