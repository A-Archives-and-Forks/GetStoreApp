    <Style x:Key="PageStyle" TargetType="Page">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <!--  页面距离控件间隔  -->
    <x:Double x:Key="PageSpacing">10</x:Double>
</ResourceDictionary>