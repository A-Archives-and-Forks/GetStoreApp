<wuxc:ContentDialog
    x:Class="GetStoreAppWebView.UI.Dialogs.ProcessFailedDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:root="using:GetStoreAppWebView.Services.Root"
    xmlns:wuxc="using:Windows.UI.Xaml.Controls"
    Title="{x:Bind root:ResourceService.GetLocalized('Dialog/WebViewProcessFailedTitle')}"
    BorderBrush="{ThemeResource ContentDialogBorderThemeBrush}"
    CloseButtonText="{x:Bind root:ResourceService.GetLocalized('Dialog/OK')}"
    Content="{x:Bind root:ResourceService.GetLocalized('Dialog/WebViewProcessFailedContent')}"
    Style="{StaticResource DefaultContentDialogStyle}"
    mc:Ignorable="d" />
