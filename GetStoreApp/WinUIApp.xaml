<Application
    x:Class="GetStoreApp.WinUIApp"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    HighContrastAdjustment="None">

    <Application.Resources>
        <ResourceDictionary>
            <x:Double x:Key="TeachingTipMinWidth">0</x:Double>
            <x:Double x:Key="TeachingTipMaxWidth">9999</x:Double>
            <Thickness x:Key="TeachingTipButtonPanelMargin">0,0,0,0</Thickness>

            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources />
                <ResourceDictionary Source="ms-appx:///Styles/WinUI/HyperlinkButton.xaml" />
                <ResourceDictionary Source="ms-appx:///Styles/WinUI/ToggleSwitch.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
